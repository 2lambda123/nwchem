title "GW Trifluoroacetamide"
start
echo

memory 2000 mb

geometry
F  -1.0459    -1.2746    -0.0047
F  -1.1324     0.6060     1.0928
F  -1.1342     0.6142    -1.0878
O   1.5154     1.1380    -0.0012
N   1.4926    -1.1655    -0.0010
C  -0.5961     0.0059    -0.0002
C   0.9005     0.0760     0.0021
H   2.5025    -1.2628    -0.0022
H   0.9511    -2.0241    -0.0008
end

basis "ao basis" spherical
 * library cc-pvdz
end

basis "cd basis" spherical
 * library cc-pvdz-rifit
end


dft
 tolerances acccoul 12
 xc xpbe96 0.55 hfexch 0.45 cpbe96 1.0
 direct
 noprint "final vectors analysis"
 grid nodisk
end

gw
 core
 eta 0.01
 states alpha occ 7
 method cdgw
 solver newton 15
end

task dft gw



dft
 noscf
end

gw
 core
 eta 0.01
 states alpha occ 7
 method cdgw
 solver graph
end

task dft gw

dft
 noscf
end

gw
 core
 first 4
 eta 0.01
 solver newton 15
 states alpha occ 4
 method cdgw
end

task dft gw

dft
 noscf
end

unset gw:first

gw
 core
 eta 0.01
 states alpha occ 7
 solver newton 15
end

task dft gw

dft; noscf; end

gw
 core
 eta 0.01
 first 4
 states alpha occ 4
 solver graph
end

task dft gw
